<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.po">
	<class
		name="TblCompanyEmployee"
		table="TBL_COMPANY_EMPLOYEE"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="EMPLOYEE_NUM"
		>
			<generator class="assigned"/>
		</id>
		
		<property
			name="name"
			column="NAME"
			type="string"
			not-null="true"
			length="40"
		/>
		<property
			name="employeeId"
			column="EMPLOYEE_ID"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="sex"
			column="SEX"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="telephone"
			column="TELEPHONE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="mobiephone"
			column="MOBIEPHONE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="email"
			column="EMAIL"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="job"
			column="JOB"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="stepment"
			column="STEPMENT"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="entryTime"
			column="ENTRY_TIME"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="rotationTime"
			column="ROTATION_TIME"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="levels"
			column="LEVELS"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="edution"
			column="EDUCATION"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="professional"
			column="PROFESSIONAL"
			type="string"
			not-null="false"
			length="25"
		/>
		<property
			name="graduate"
			column="GRADUATE"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="birthday"
			column="BIRTHDAY"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="address"
			column="ADDRESS"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="permanentAddress"
			column="PERMANENT_ADDRESS"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="identityNum"
			column="IDENTITY_NUM"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="contactTelephone"
			column="CONTACT_TELEPHONE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="contact"
			column="CONTACT"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="overtime"
			column="OVER_TIME"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="fax"
			column="FAX"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="parentId"
			column="PARENT_EMPNUM"
			type="string"
			not-null="false"
			length="15"
		/>
	</class>	
</hibernate-mapping>